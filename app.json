{
  "expo": {
    "name": "TrustBase",
    "slug": "TrustBase",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png", // 设置应用程序图标
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png", // 设置自适应图标的前景图像
        "backgroundColor": "#ffffff" //置自适应图标的背景颜色
      },
      "package": "com.shaoping.TrustBase"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png" // 设置网页图标
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png", // 启动屏幕图像
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000" // 启动屏幕背景颜色
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a4c7035b-55bc-4f95-8f73-d9c7a6daf274"
      }
    }
  }
}